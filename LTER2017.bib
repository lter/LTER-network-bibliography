@article{knox_decoupled_2017,
	title = {Decoupled responses of soil bacteria and their invertebrate consumer to warming, but not freeze-thaw cycles, in the {Antarctic} {Dry} {Valleys}},
	volume = {20},
	issn = {1461-023X},
