@article{zambrano_local_2017,
	title = {Local neighbourhood and regional climatic contexts interact to explain tree performance},
	volume = {284},
	issn = {0962-8452},
	shorttitle = {Local neighbourhood and regional climatic contexts interact to explain tree performance},
	url = {://WOS:000405148800008},
	doi = {10.1098/rspb.2017.0523},
	abstract = {Tree neighbourhood modelling has significantly contributed to our understanding of the mechanisms structuring communities. Investigations into the impact of neighbouring crowding on tree performance have generally been conducted at local scales, missing important regional-scale context such as the suitability of the climate for each species. Favourable climates may enhance tree performance, but this may come at the cost of increased neighbourhood crowding and competition negatively impacting survival and growth. Through the synthesis of continental-scale forest inventory and trait datasets from the northeast USA and Puerto Rico we present an analytical approach that elucidates the important interactions between local competitive and regional climatic contexts. Our results show strong asymmetries in competitive interactions and significant niche differences that are dependent on habitat suitability. The strong interaction between local neighbourhood and regional climate highlights the need for models that consider the interaction between these two processes that have been previously ignored.},
	language = {English},
	number = {1855},
	journal = {Proceedings of the Royal Society B-Biological Sciences},
	author = {Zambrano, J. and Marchand, P. and Swenson, N. G.},
	month = may,
	year = {2017},
